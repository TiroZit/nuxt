{
	"name": "nuxt-app",
	"type": "module",
	"private": true,
	"engines": {
		"node": ">=18.13"
	},
	"scripts": {
		"build": "npm run lint && nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"deploy": "npm run generate && gh-pages -d dist",
		"lint": "eslint --fix . && stylelint --fix **/*.{sass,vue}"
	},
	"browserslist": [
		">1%",
		"not op_mini all",
		"not ie 11",
		"last 3 chrome version",
		"last 3 firefox version",
		"last 3 safari version"
	],
	"dependencies": {
		"@nuxt/ui": "2.10.0",
		"tailwindcss": "3.3.5",
		"vue": "3.3.8",
		"vue-router": "4.2.5"
	},
	"devDependencies": {
		"@antfu/eslint-config": "1.1.1",
		"@nuxt/devtools": "1.0.2",
		"@nuxt/devtools-ui-kit": "1.0.2",
		"@stylistic/eslint-plugin": "1.4.0",
		"autoprefixer": "10.4.16",
		"eslint": "8.53.0",
		"gh-pages": "6.0.0",
		"nuxt": "3.8.1",
		"nuxt-mongoose": "1.0.3",
		"sass": "1.69.5",
		"stylelint": "15.11.0",
		"stylelint-config-html": "1.1.0",
		"stylelint-config-recommended-scss": "13.1.0",
		"stylelint-config-recommended-vue": "1.5.0",
		"stylelint-order": "6.0.3",
		"sugarss": "4.0.1"
	}
}
